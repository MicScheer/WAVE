      use bunchmod
      use wbetaf90m
      use souintmod

C--- EVALUATE INTEGRALES FOR A SINGLE SOURCE
C---- RESULTS ARE STORE IN AFREQ AND SPECPOW

      IMPLICIT NONE

+SEQ,CMPARA.
+SEQ,CONTRL.
+SEQ,MYFILES.
+SEQ,PHYCON.
+SEQ,TRACK.
+seq,sourcef90.
+SEQ,COLLI.
+SEq,observf90.
+SEQ,SPECT.
+SEQ,FREQS.
+SEQ,AMPLI.
+SEQ,B0SCGLOB.
